<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <template id="assets_backend" name="simple_library assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script src="/simple_library/static/src/js/field_widget.js" type="text/javascript" />
            <script src="/simple_library/static/src/js/simple_library_tour.js" type="text/javascript"/>
            <link href="/simple_library/static/src/scss/field_widget.scss" rel="stylesheet" type="text/scss" />
        </xpath>
    </template>

    <template id="qunit_suite" name="colorpicker test" inherit_id="web.qunit_suite">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/simple_library/static/tests/colorpicker_tests.js"/>
        </xpath>
    </template>

</odoo>